{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "light-controller",
  "mainBinaryName": "light-controller",
  "version": "0.1.0",
  "identifier": "com.tauri.light-controller",
  "plugins": {},
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "decorations": false,
        "fullscreen": false,
        "resizable": false,
        "title": "LC 55",
        "width": 300,
        "height": 368,
        "x": 1850,
        "y": 650,
        "useHttpsScheme": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}